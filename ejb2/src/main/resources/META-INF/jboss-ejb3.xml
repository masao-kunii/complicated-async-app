<jboss:ejb-jar xmlns:jboss="http://www.jboss.com/xml/ns/javaee"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="urn:clustering:1.0"
	xmlns:jee="http://java.sun.com/xml/ns/javaee"
	xmlns:mdb="urn:resource-adapter-binding"
	xsi:schemaLocation="http://www.jboss.com/xml/ns/javaee http://www.jboss.org/j2ee/schema/jboss-ejb3-2_0.xsd http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
	version="3.1" impl-version="2.0">
	<enterprise-beans>
		<message-driven>
			<ejb-name>TestExt1MDB</ejb-name>
			<ejb-class>complicated.async.app.mdb.TestExt1MDB</ejb-class>
			<activation-config>
				<activation-config-property>
					<activation-config-property-name>destination</activation-config-property-name>
					<activation-config-property-value>${queues.ext.1.testQueue:testMQueue1}</activation-config-property-value>
				</activation-config-property>
				<activation-config-property>
					<activation-config-property-name>destinationType</activation-config-property-name>
					<activation-config-property-value>javax.jms.Queue</activation-config-property-value>
				</activation-config-property>
			</activation-config>
		</message-driven>
		<message-driven>
			<ejb-name>TestExt5MDB</ejb-name>
			<ejb-class>complicated.async.app.mdb.TestExt5MDB</ejb-class>
			<activation-config>
				<activation-config-property>
					<activation-config-property-name>destination</activation-config-property-name>
					<activation-config-property-value>${queues.ext.5.testQueue:testMQueue5}</activation-config-property-value>
				</activation-config-property>
				<activation-config-property>
					<activation-config-property-name>destinationType</activation-config-property-name>
					<activation-config-property-value>javax.jms.Queue</activation-config-property-value>
				</activation-config-property>
			</activation-config>
		</message-driven>
		<message-driven>
			<ejb-name>TestExt7MDB</ejb-name>
			<ejb-class>complicated.async.app.mdb.TestExt7MDB</ejb-class>
			<activation-config>
				<activation-config-property>
					<activation-config-property-name>destination</activation-config-property-name>
					<activation-config-property-value>${queues.ext.7.testQueue:testMQueue7}</activation-config-property-value>
				</activation-config-property>
				<activation-config-property>
					<activation-config-property-name>destinationType</activation-config-property-name>
					<activation-config-property-value>javax.jms.Queue</activation-config-property-value>
				</activation-config-property>
			</activation-config>
		</message-driven>
	</enterprise-beans>
	<jee:assembly-descriptor>
		<mdb:resource-adapter-binding>
			<jee:ejb-name>TestExt1MDB</jee:ejb-name>
			<mdb:resource-adapter-name>activemq-rar-5.7.0.rar</mdb:resource-adapter-name>
		</mdb:resource-adapter-binding>
		<mdb:resource-adapter-binding>
			<jee:ejb-name>TestExt5MDB</jee:ejb-name>
			<mdb:resource-adapter-name>activemq-rar-5.7.0-5.rar</mdb:resource-adapter-name>
		</mdb:resource-adapter-binding>
		<mdb:resource-adapter-binding>
			<jee:ejb-name>TestExt7MDB</jee:ejb-name>
			<mdb:resource-adapter-name>activemq-rar-5.7.0-7.rar</mdb:resource-adapter-name>
		</mdb:resource-adapter-binding>
	</jee:assembly-descriptor>

</jboss:ejb-jar>